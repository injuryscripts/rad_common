.row
  .col-lg-6
    = f.input filter.start_input, as: :date, html5: true, label: filter.start_input_label,
              input_html: {  value: search.search_params[filter.start_input], data: { default: search.search_params[filter.start_input] } }
  .col-lg-6
    = f.input filter.end_input, as: :date, html5: true, label: filter.end_input_label,
              input_html: {  value: search.search_params[filter.end_input], data: { default: search.search_params[filter.end_input] } }
